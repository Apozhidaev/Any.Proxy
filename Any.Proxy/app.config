<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <sectionGroup name="anylog">
      <section name="file" type="Any.Logs.Loggers.Files.Configuration.FileLoggerSection, Any.Logs, Version=1.0.0.0, Culture=neutral" />
    </sectionGroup>
    <section name="proxy" type="Any.Proxy.Configuration.ProxySection, Any.Proxy, Version=1.0.0.0, Culture=neutral" />
  </configSections>
  <anylog>
    <file>
      <loggers>
        <logger methods="Error" path="[cdir]\Errors\" />
        <logger methods="Info" path="[cdir]\Info\" />
      </loggers>
    </file>
  </anylog>
  <proxy>
    <!--<portMap>
      <module name="test" fromHost="localhost" fromPort="63342" toHost="localhost" toPort="61093" />
    </portMap>-->
    <!--<http>
      <module name="test" port="50000" />
    </http>-->
    <httpAgent>
      <module name="test" port="50000" url="http://localhost:52222/" />
    </httpAgent>
    <httpService>
      <module name="test" prefixes="http://localhost:52222/" />
    </httpService>
  </proxy>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>